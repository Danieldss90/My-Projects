#include<stdio.h>
#include<stdlib.h>
 
main(){
 int testes, numero, verificanumero,  verificatestes=0, modulo;
  
 scanf("%d", &testes);
 
 while(verificatestes < testes){
    scanf("%d", &numero);
    verificanumero=2;
        while(numero%verificanumero != 0 && verificanumero < numero){
            modulo = numero%verificanumero;
            verificanumero++;
         }
          
         if(verificanumero == numero && modulo!=0){
            printf("%d eh primo\n", numero);
         }else
         printf("%d nao eh primo\n", numero);
 
     
    verificatestes++;   
 }
 
 
 
 
 
 
 
 
system("pause>>null");
}
